package com.eaglesandcobras.common.data{		public class DataObject{		private var _type:String;		private var _call:String;		private var _loc:String;		private var _params:Object;				public static const AMF:String = "AMF";		public static const XML:String = "XML";		public static const RSS:String = "RSS";				public function DataObject($type:String=null, $call:String=null, $params:Object=null, $loc:String=null):void {			_type = $type;			_call = $call;			_loc = $loc;			_params = $params;		}				/*****************************		TYPE		*****************************/		public function set type($s:String):void {			_type = $s;		}				public function get type():String {			return _type;		}				/*****************************		CALL		*****************************/		public function set call($s:String):void {			_call = $s;		}				public function get call():String {			return _call;		}				/*****************************		PARAMS		*****************************/		public function set params($p:Object):void {			_params = $p;		}				public function get params():Object {			return _params;		}				/*****************************		LOCATION		*****************************/		public function set location($s:String):void {			_loc = $s;		}				public function get location():String {			return _loc;		}			}}