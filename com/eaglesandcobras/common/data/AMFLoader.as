package com.eaglesandcobras.common.data{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.net.NetConnection;	import flash.net.Responder;		public class AMFLoader extends EventDispatcher{		private var _call:String;		private var _loc:String;		private var _params:Object;		private var _data:Object;				public function AMFLoader($call:String, $location:String, $params:Object){			super();						_call = $call;			_loc = $location;			_params = $params;						init();		}				private function init():void{			var _nc:NetConnection = new NetConnection();			_nc.connect(_loc);						var _responder:Responder = new Responder(onResult, onError);						_nc.call(_call, _responder, _params);		}				private function onResult(e:Object):void {			_data = e;						this.dispatchEvent( new Event (Event.COMPLETE) );		}				private function onError(e:Object):void {			trace (e);		}				/*****************************		GET Data		*****************************/		public function get data():Object {			return _data;		}	}}