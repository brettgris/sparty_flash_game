package com.eaglesandcobras.common.util.graphics {	import flash.display.Sprite;		public class Mask extends Sprite {		public var _settings:Object = {width:800, height:600};		protected var square:Sprite;				public function Mask(_s:Object):void {			_settings = _s;			drawMask();		}				private function drawMask():void {			square = new Sprite();			addChild(square);			square.graphics.beginFill(0x000000);			square.graphics.drawRect(0,0,_settings.width,_settings.height);			square.graphics.endFill();		}	}}