package com.eaglesandcobras.common.util.math {	public class Random {				public static function sequence($length:Number):Array {			var shuffleArr = new Array();			var returnArr = new Array();						for(var i:uint = 0; i< $length; i++){				var randKey:int = Math.random()*($length*1000);				shuffleArr[i] = {id:i,key:randKey};				shuffleArr.sortOn("key",Array.NUMERIC);			}						for (var a:uint=0;a< shuffleArr.length;a++) {				returnArr.push ( shuffleArr[a].id );			}						return returnArr;		}	}}