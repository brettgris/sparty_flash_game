package com.eaglesandcobras.common.events{	import flash.events.Event;		public class LoaderEvent extends Event	{		public var params:Object;				public static const DATA_READY:String = "DATA_READY";		public static const DATA_ADDED:String = "DATA_ADDED";		public static const CONTENT_READY:String = "CONTENT_READY";				public function LoaderEvent($type:String, $params:Object=null, $bubbles:Boolean=false, $cancelable:Boolean=false)		{			super($type, $bubbles, $cancelable);			this.params = $params;		}				public override function clone():Event {						return new LoaderEvent(type, this.params, bubbles, cancelable);					}		public override function toString():String {						return formatToString("LoaderEvent", "params", "type", "bubbles", "cancelable");					}	}}