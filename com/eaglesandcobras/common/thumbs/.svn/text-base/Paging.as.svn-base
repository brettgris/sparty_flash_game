package com.eaglesandcobras.common.thumbs{		public class Paging extends Object{		private var _current:int = 0;		private var _currentPage:int = 0;		private var _pages:int = 1;		private var _totalItems:int = 1;		private var _itemsPerPage:int = 1;				public function Paging(){		}				/*****************		Current		*****************/		public function set current($b:int):void {			_current = $b;		}				public function get current():int {			return _current;		}				/*****************		Current Page		*****************/		public function set currentPage($b:int):void {			_currentPage = $b;		}				public function get currentPage():int {			return _currentPage;		}				/*****************		Number of Pages		*****************/		public function set totalPages($b:int):void {			_pages = $b;		}				public function get totalPages():int {			return _pages;		}				/*****************		Total Items		*****************/		public function set totalItems($b:int):void {			_totalItems = $b;		}				public function get totalItems():int {			return _totalItems;		}				/*****************		Items Per Page		*****************/		public function set itemsPerPage($b:int):void {			_itemsPerPage = $b;		}				public function get itemsPerPage():int {			return _itemsPerPage;		}	}}