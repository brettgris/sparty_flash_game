package com.eaglesandcobras.common.mvc{	import flash.events.EventDispatcher;	import com.eaglesandcobras.common.events.ModelEvent;		public class Model extends EventDispatcher{		public function Model():void {			super();		}				public function dataStarted(e:ModelEvent){			this.dispatchEvent(e);		}				public function dataComplete(e:ModelEvent){			this.dispatchEvent(e);		}				public function dataError(e:ModelEvent){			this.dispatchEvent(e);		}				public function dataProgress(e:ModelEvent){			this.dispatchEvent(e);		}	}}